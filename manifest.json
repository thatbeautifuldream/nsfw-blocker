{
  "manifest_version": 2,
  "name": "NSFW Blocker",
  "version": "0.0.1",
  "browser_action": {
    "default_title": "NSFW Blocker"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ]
}
